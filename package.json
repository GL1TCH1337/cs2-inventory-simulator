{
  "name": "@ianlucas/cs2-inventory-simulator",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "deploy": "npx @ianlucas/deploy@latest",
    "dev": "remix vite:dev",
    "format": "prettier . --write",
    "script": "tsx",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "upgrade": "npx npm-check-updates --reject \"@types/node isbot\" --target minor -u"
  },
  "dependencies": {
    "@floating-ui/react": "^0.26.9",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@ianlucas/cslib": "^12.1.1",
    "@ianlucas/remix-auth-steam": "^2.0.2",
    "@prisma/client": "^5.11.0",
    "@remix-run/node": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "@remix-run/serve": "^2.8.1",
    "@uidotdev/usehooks": "^2.4.1",
    "clsx": "^2.1.0",
    "isbot": "^3.6.8",
    "prisma": "^5.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.6.0",
    "remix-typedjson": "^0.4.1",
    "remix-utils": "^7.5.0",
    "steamapi": "^3.0.8",
    "tiny-invariant": "^1.3.3",
    "tsx": "^4.7.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.8.1",
    "@types/node": "^18.19.8",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
